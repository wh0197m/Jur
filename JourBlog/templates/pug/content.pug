doctype html
html
  include head.pug
  body
    div.main
      div.container
        include:markdown-it #{Article}
      div.nav
        div.sub
          ul
            - var lists = #{Contents}
            each item in lists
              li
                a(href=item.link) #{item.title}
        div.menu
          ul
            - var Navs = #{Navigators}
            each nav in Navs
              li
                a(id=nav href='javascript:void(0)') #{nav}
    include copyright.pug

